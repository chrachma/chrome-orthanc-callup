{
    "name": "Orthanc Study Callup",
    "description": "When activated in Orthanc, it opens a new tab with DICOM Study Instance UID in a customizable URL, e.g. for callup in an external viewer",
    "version": "0.2",
    "manifest_version": 3,
    "options_page": "options.html",
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
      }
    },
    "icons": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    }
  }